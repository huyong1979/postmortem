#execute the script if 1 to 0
record(calcout,"SR-APHLA{RF-CFD2}PM-Calc_") {
    #field(INPA,"SR:C23-BI{BPM:10}PM:Status-I CP")
    field(INPA,"SR-RF{CFD:2-RAM}Cmd:Time CP")
    field(CALC,"A")
    field(OUT, "SR-APHLA{RF}PM-Cmd.PROC")
    field(ODLY, "60")
    field(OOPT,"Transition To Zero")
}

record(bo, "SR-APHLA{RF-CFD2}PM-Cmd") {
    field(DTYP,"Launcher")
    field(OUT, "@python /epics/iocs/pm-RF/save_pmdata.py RF-CFD2")
    field(ZNAM,"Idle")
    field(ONAM,"Setting")
}

record(bo, "SR-APHLA{RF-CFD2}PM:TestEnabled-Cmd") {
    field(ZNAM, "NO")
    field(ONAM, "YES")
    field(VAL,  "1")
    field(PINI, "YES")
    #field(HIGH, "10")
}

record(waveform, "SR-APHLA{RF-CFD2}PM:Status-Wf") {
    field(DESC,"Status Message")
    field(NELM,"500")
    field(FTVL,"CHAR")
}
